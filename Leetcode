var l1 = [1,0,1]
var l2 = [2,3,4]
function addTwoNumbers(l1,l2){
var result = 0;
    for (var i = l1.length - 1; i >= 0; i--){
	result += l1[i]*Math.pow(10,i)
    }
    for (var i = l2.length - 1; i >= 0; i--){
	result += l2[i]*Math.pow(10,i)
    }
	var a = result;
	var result2 = Array.from(a.toString(), (val) => Number(val));
	for (var i = result2.length - 2; i >= 0; i--){
	result2.push(ListNode[i])
    }
	for (var i = 0; i < 2 ; i++){
	result2.shift(ListNode[i])
    }
return result2
}
